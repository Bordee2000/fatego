    const classIndex = this.select_filter.findIndex(e => e == key);\n          console.log(classIndex);\n          this.select_filter.splice(classIndex, 1);\n        }\n      }\n      console.log(this.select_filter);\n    }\n  }\n};\n</script>\n\n\n"]}]}